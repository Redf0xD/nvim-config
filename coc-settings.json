{
  "prettier.arrowParens": "avoid",
  "prettier.bracketSpacing": true,
  "prettier.embeddedLanguageFormatting": "auto",
  "prettier.htmlWhitespaceSensitivity": "css",
  "prettier.insertPragma": false,
  "prettier.jsxSingleQuote": false,
  "prettier.printWidth": 80,
  "prettier.proseWrap": "preserve",
  "prettier.quoteProps": "as-needed",
  "prettier.requirePragma": false,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  "prettier.trailingComma": "none",
  "prettier.useTabs": false,
  "prettier.vueIndentScriptAndStyle": false,
  "emmet.includeLanguages": {
    "javascript": [
      "javascriptreact",
      "javascript"
    ],
    "vue-html": "html",
    "svelte": "html",
    "django": "html"
  },
  "typescript.autoClosingTags": true,
  "javascript.autoClosingTags": true,
  "eslint.packageManager": "pnpm",
  "eslint.nodePath": "/usr/bin/node",
  "eslint.autoFixOnSave": true,
  "css.validate": true,
  "less.validate": false,
  "scss.validate": false,
  "Lua.diagnostics.globals": [
    "vim",
    "use"
  ],
  "coc.source.emoji.filetypes": [
    "markdown",
    "html",
    "javascriptreact"
  ],
  "suggest.completionItemKindLabels": {
    "method": "  ",
    "function": "  ",
    "variable": "[]",
    "field": "  ",
    "typeParameter": "<>",
    "constant": "  ",
    "class": "  ",
    "interface": " 蘒",
    "struct": "  ",
    "event": "  ",
    "operator": "  ",
    "module": "  ",
    "property": "  ",
    "enum": " 練",
    "reference": "  ",
    "keyword": "  ",
    "file": "  ",
    "folder": " ﱮ ",
    "color": "  ",
    "unit": " 塞 ",
    "snippet": " ",
    "text": "  ",
    "constructor": "  ",
    "value": "  ",
    "enumMember": "  ",
    "default": "  "
  },
  "coc.source.file.priority": 60,
  "coc.source.buffer.priority": 50,
  "suggest.triggerCompletionWait": 30,
  "suggest.autoTrigger": "always",
  "suggest.localityBonus": true,
  "suggest.enablePreselect": true,
  "suggest.removeDuplicateItems": true,
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "suggest.defaultSortMethod": "alphabetical",
  "suggest.triggerAfterInsertEnter": false,
  "suggest.noselect": false,
  "diagnostic.errorSign": " ",
  "diagnostic.warningSign": " ",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": " ",
  "coc.preferences.formatOnSave": true,
  "languageserver": {
    "ocaml-lsp": {
      "command": "opam",
      "args": [
        "config",
        "exec",
        "--",
        "ocamllsp"
      ],
      "filetypes": [
        "ocaml",
        "reason"
      ]
    }
  },
  "Lua.telemetry.enable": false,
  "snippets.ultisnips.pythonPrompt": false,
  "coc.preferences.extensionUpdateCheck": "daily",
  "markdown-preview-enhanced.liveUpdate": true,
  "svelte.enable-ts-plugin": true
}
